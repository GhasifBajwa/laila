body {
  margin: 0;
  padding: 0;
}

nav {
  position: fixed;
  width: 90%;
  height: 60px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  z-index: 1000;
}

.logo {
  width: 102.23px;
  height: 45.67px;
  margin-left: 10px;
  margin-top: 10px;
}

.nav-center {
  display: flex;
  gap: 40px;
  font-size: 13px;
  text-transform: uppercase;
  letter-spacing: 0.5px;
  transform: translateX(450px);
}

.nav-center a {
  text-decoration: none;
  color: #000;
  font-weight: 500;
  font-size: 14px;
  font-family: GT Cinetype Trial;
  line-height: 30px;
}

.nav-center a:hover {
  color: #999999 !important;
}

.nav-icons {
  display: flex;
  gap: 25px;
  height: 18.57px;
  width: 19.12px;
  color: #000;
  cursor: pointer;
}

.nav-scrolled-white a {
  color: white !important;
}

.nav-scrolled-white .nav-icons img {
  color: white !important;
}

.search-input {
  position: absolute;
  top: 10px;
  right: 20px;
  width: 363px;
  height: 42px;
  padding: 0px 15px;
  font-size: 12px;
  font-family: GT Cinetype Trial;
  font-weight: 900;
  background-color: #EDEDEF;
  border: 1px solid #EDEDEF !important;
  display: none;
  opacity: 0;
  transform: translateX(100%);
  transition: opacity 0.3s ease, transform 0.3s ease;
}

.search-input.visible {
  opacity: 1;
  transform: translateX(0);
}

.hero-wrapper {
  height: 100vh;
  position: relative;
  overflow: hidden;
}

.hero {
  position: fixed;
  top: 0;
  left: 0;
  height: 100vh;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 10;
}

.img-container {
  width: 60vw;
  height: 60vh;
  transition: transform 0.1s ease-out;
  transform: scale(1);
  overflow: hidden;
}

.img-container img {
  width: 100%;
  height: 100%;
  display: block;
}

.hero-text {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 5;
  pointer-events: none;
  font-family: GT Cinetype Trial;
}

.text-left,
.text-right {
  font-size: 3rem;
  font-weight: 800;
  color: white;
  opacity: 0;
  transition: all 0.5s ease-out;
  white-space: nowrap;
}

.text-left {
  transform: translateX(-100%);
  margin-right: 20px;
}

.text-right {
  transform: translateX(100%);
  margin-left: 20px;
}

.active .text-left {
  transform: translateX(0);
  opacity: 1;
}

.active .text-right {
  transform: translateX(0);
  opacity: 1;
}

.spacer {
  height: 100vh;
}


/* --------- Responsive Media Queries --------- */

/* For screens <= 1366px (Common laptop size) */
@media (max-width: 1366px) {
  .nav-center {
    gap: 30px;
    font-size: 12px;
    transform: translateX(350px);
  }

  .nav-center a {
    font-size: 13px;
  }

  .search-input {
    width: 280px;
    height: 38px;
    font-size: 11px;
    right: 15px;
    top: 8px;
  }

  .img-container {
    width: 55vw;
    height: 55vh;
  }

  .text-left,
  .text-right {
    font-size: 2.5rem;
  }
}

/* For screens <= 1280px */
@media (max-width: 1280px) {
  .nav-center {
    gap: 25px;
    transform: translateX(300px);
  }

  .nav-center a {
    font-size: 12px;
  }

  .search-input {
    width: 250px;
    height: 36px;
    font-size: 11px;
    right: 12px;
    top: 7px;
  }

  .img-container {
    width: 50vw;
    height: 50vh;
  }

  .text-left,
  .text-right {
    font-size: 2.2rem;
  }
}

/* For screens <= 1024px (Small laptops, large tablets) */
@media (max-width: 1024px) {
  nav {
    width: 95%;
  }

  .nav-center {
    gap: 20px;
    font-size: 11px;
    transform: translateX(200px);
  }

  .nav-center a {
    font-size: 11px;
  }

  .search-input {
    width: 220px;
    height: 34px;
    font-size: 10px;
    right: 10px;
    top: 6px;
  }

  .img-container {
    width: 45vw;
    height: 45vh;
  }

  .text-left,
  .text-right {
    font-size: 1.8rem;
    margin: 0 10px;
  }
}

/* For screens >= 1920px (Large desktops) */
@media (min-width: 1920px) {
  nav {
    width: 75%;
  }

  .nav-center {
    gap: 50px;
    font-size: 15px;
    transform: translateX(550px);
  }

  .nav-center a {
    font-size: 16px;
  }

  .search-input {
    width: 400px;
    height: 44px;
    font-size: 13px;
    right: 25px;
    top: 12px;
  }

  .img-container {
    width: 65vw;
    height: 65vh;
  }

  .text-left,
  .text-right {
    font-size: 3.5rem;
  }
}


